{
    "name": "omniholdingcompany/cashier-authorize",
    "description": "Laravel Cashier-Authorize provides an expressive, fluent interface to Authorize.net's subscription billing services.",
    "keywords": ["laravel", "stripe", "billing","authorize.net","payment module"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shahid Karimi",
            "email": "karimi.shahid@gmail.com"
        },
        {
            "name": "Chris French",
            "email": "chris.french@omniholding.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/goetas/serializer.git"
        }
    ],
        "require": {
            "authorizenet/authorizenet": "^1.9.2",
            "dompdf/dompdf": "^0.6.1",
            "illuminate/container": "^5.2|^5.1",
            "illuminate/database": "~5.1",
            "illuminate/support": "~5.1",
            "nesbot/carbon": "~1.0",
            "php": ">=7.1.0",
            "symfony/http-kernel": "~2.7|~3.0",
            "moneyphp/money": "^3.0"
        },
        "require-dev": {
            "goetas-webservices/xsd-reader": "0.2.*",
            "goetas-webservices/xsd2php": "0.3.*",
            "illuminate/http": "~5.1",
            "illuminate/routing": "~5.1",
            "illuminate/view": "~5.1",
            "mockery/mockery": "~0.9",
            "phpunit/phpunit": "~4.0",
            "vlucas/phpdotenv": "~2.0"
        },
    "autoload": {
        "psr-4": {
            "Laravel\\CashierAuthorizeNet\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "7.0.x-dev"
        }
    }
}
